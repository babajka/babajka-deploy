server {
	listen 80 default_server;
	listen [::]:80 default_server;

	server_name wir.by www.wir.by stub.wir.by www.stub.wir.by ipv6.wir.by www.ipv6.wir.by;

	location / {
		root /home/wir-dev/babajka-markup/dist/stubs;
		index development.html;
		try_files $uri $uri/ =404;
	}

	location /robots.txt {
		root /home/wir-dev/babajka-markup/dist/static/search-engines/prod;
	}

	location /static/ {
		root /home/wir-dev/babajka-markup/dist;
		autoindex on;
		try_files $uri $uri/ =404;
	}
}
